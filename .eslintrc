{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": [
      "error", 
      {
        "vars": "local",
        "args": "none"
      }
    ],
    "linebreak-style": "off",
    "react/jsx-filename-extension": "off",
    "import/prefer-default-export": "off",
    "react/destructuring-assignment": "off",
    "no-nested-ternary": "off",
    "no-useless-escape": "off"
  }
}